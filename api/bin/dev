#!/usr/bin/env node
const { exec } = require('child_process');

function cmd(command) {
  exec(command, (_error, stdout, stderr) => {
    console.log(stdout)
    console.error(stderr)
  })
}

function transpile() {
  cmd('yarn build')
}

function lint() {
  cmd('yarn lint')
}

function run_server() {
  cmd('yarn start')
}

lint()
transpile()
run_server()